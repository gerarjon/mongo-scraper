<div class="row">
  <h1>Read the Latest Articles from 'The Onion News Network'</h1>
</div>
<div class="row">
  <div class="col s2"></div>
  <div class="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="btnRead">Read Article</a>
      </div>
  </div>
</div>